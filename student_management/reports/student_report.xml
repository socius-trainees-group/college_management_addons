<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="student_management.report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Student Report</h2>
                <table>
                    <tr>
                        <th>Name:&#160; </th>
                        <td> <t t-esc="doc.name"/> </td>
                    </tr>
                    <tr>
                        <th>Age:&#160; </th>
                        <td> <t t-esc="doc.age"/> </td>
                    </tr>
                    <tr>
                        <th>Date_Of_Birth:&#160; </th>
                        <td> <t t-esc="doc.date_of_birth"/> </td>
                    </tr>
                    <tr>
                        <th>Mail Id:&#160;</th>
                        <td> <t t-esc="doc.mail_id"/> </td>
                    </tr>
                    <tr>
                        <th>Department:&#160;</th>
                        <td> <t t-esc="doc.department"/> </td>
                    </tr>
                </table>
            </t>
        </t>
    </template>
</odoo>