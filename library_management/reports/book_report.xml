<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <template id="library_management.book_report">


        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Book details</h2>
                <table >
                    <tr>
                        <th>Book Name</th>
                        <th>Book Number</th>
                        <th>Author Name</th>
                    </tr>
                    <tr>
                        <td><t t-esc="doc.name"/></td>
                        <td><t t-esc="doc.number"/></td>
                        <td><t t-esc="doc.author_id"/></td>


                    </tr>
                </table>
            </t>
        </t>
    </template>


</odoo>